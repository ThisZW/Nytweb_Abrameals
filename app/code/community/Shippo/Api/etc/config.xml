<config>
	<modules>
		<Shippo_Api>
			<version>0.0.1</version>
		</Shippo_Api>
	</modules>
        <admin>
          <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Shippo_Api after="Mage_Adminhtml">Shippo_Api</Shippo_Api>
                    </modules>
                </args>
            </adminhtml>
          </routers>
        </admin>
	<default>
		<shippoapi>
			<config>
				<realm>Shippo</realm>
			</config>
		</shippoapi>
	</default>
</config>

